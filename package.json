{
    "name": "mini-panda",
    "displayName": "mini-panda",
    "description": "mini-panda program language for embed system (8 bit and 32 bit)",
    "version": "0.0.1",
    "publisher": "sang",
    "engines": {
        "vscode": "^1.46.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "mini-panda",
            "aliases": ["mpd", "mini-panda"],
            "extensions": [".mpd"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "mini-panda",
            "scopeName": "source.mpd",
            "path": "./syntaxes/mini-panda.tmLanguage.json"
        }]
    }
}